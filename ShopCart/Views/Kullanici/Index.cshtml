
@{
    ViewBag.Title = "KUllanicilar";

}


<body>
    <div class="container">
        <div id="gototop"> </div>
        <!--
        Body Section
        -->
        <div class="row">
            <div class="span12">
                <div class="well well-small">
                    <h1>Kullanicilar Sayfasi <small class="pull-right"> Henuz Kayitli @Model.Count tane kullanici  var </small></h1>
                    <hr class="soften" />

                    <table class="table table-bordered table-condensed">
                        <thead>
                            <tr>
                                <th>KullaniciAdi</th>
                                <th>Kullanici Soyadi</th>
                                <th>Durumu</th>
                                <th>Kullanici Maili </th>
                                <th>KullaniciRolu</th>
                                <th>Olusturma Tarihi</th>
                            </tr>
                        </thead>
                        <tbody>
                            @foreach (ShopCart.Models.Kullanicilar k in Model)
                            {
                                <tr>
                                    <td>@k.kullaniciAdi</td>
                                    <td> @k.kullaniciSoyadi </td>
                                    <td><span class="shopBtn"><span class="icon-ok"></span></span> </td>
                                    <td>@k.kullaniciMail</td>
                                    <td>@k.kullaniciRolu</td>
                                    <td>@k.olusturmaTarihi</td>
                                </tr>
                            }

                        </tbody>
                    </table><br />

                </div>
            </div>
        </div>

        <!-- Placed at the end of the document so the pages load faster -->
        <script src="~/Contents/assets/js/jquery.js"></script>
        <script src="~/Contents/assets/js/bootstrap.min.js"></script>
        <script src="~/Contents/assets/js/jquery.easing-1.3.min.js"></script>
        <script src="~/Contents/assets/js/jquery.scrollTo-1.4.3.1-min.js"></script>
        <script src="~/Contents/assets/js/shop.js"></script>
</body>
</html>

}

